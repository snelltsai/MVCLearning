@{
    ViewBag.Title = "CreatePoke";
}

<h2>CreatePoke</h2>

<form method="post" action="/Home/CreatePoke">

    <div class="form-group row">
        <label for="inputCharName" class="col-sm-2">ID</label>
        <div class="col-sm-10">
            <input type="number" id="inputPokeID" name="PokeID" class=" form-control" />
        </div>
    </div>

    <div class="form-group row">
        <label for="inputCharName" class="col-sm-2">Name</label>
        <div class="col-sm-10">
            <input type="text" id="inputPokeName" name="PokeName" class=" form-control" />
        </div>
    </div>
    <input class="btn btn-default" type="submit" value="create" />
</form>

