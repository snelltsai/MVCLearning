@model IEnumerable<ZachT.Models.tToDo>

@{
    ViewBag.Title = "ToDo";
}

<h2>ToDo</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fLevel)
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.TODO) {
    <tr>
        <td>
           @item.fLevel
        </td>
        <td>
            @item.fTitle
        </td>
      
    </tr>
}

</table>
